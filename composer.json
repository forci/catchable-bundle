{
    "name": "forci/catchable-bundle",
    "type": "symfony-bundle",
    "description": "A bundle to collect your unhandled exceptions",
    "keywords": ["exception bundle", "forci", "wucdbm"],
    "authors": [
        {
            "name": "Tatyana Mincheva",
            "email": "tatjana@forci.com",
            "role": "Developer"
        },
        {
            "name": "Martin Kirilov",
            "email": "wucdbm@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "symfony/framework-bundle": "~3.0|~4.0",
        "symfony/form": "~3.0|~4.0",
        "doctrine/orm": "~2.5|~2.6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.8",
        "wucdbm/php-cs-fixers": "~0.3"
    },
    "autoload": {
        "psr-4": { "Forci\\Bundle\\Catchable\\": "src" }
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "7.1.3"
        }
    },
    "license": "MIT"
}