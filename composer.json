{
    "name": "forci/catchable-bundle",
    "type": "symfony-bundle",
    "description": "A bundle to collect your unhandled exceptions",
    "keywords": ["exception bundle", "forci", "wucdbm"],
    "authors": [
        {
            "name": "Tatyana Mincheva",
            "email": "tatjana@forci.com",
            "role": "Developer"
        },
        {
            "name": "Martin Kirilov",
            "email": "wucdbm@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/framework-bundle": "~5.0|~6.0",
        "symfony/form": "~5.0|~6.0",
        "monolog/monolog": "^1.25.3",
        "doctrine/doctrine-bundle": "~1.12|~2.0",
        "doctrine/orm": "~2.6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.8|^3.0",
        "wucdbm/php-cs-fixers": "~0.3"
    },
    "autoload": {
        "psr-4": { "Forci\\Bundle\\Catchable\\": "src" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "license": "MIT"
}